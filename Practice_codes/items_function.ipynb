      "source": [
        "#python by using items() function, easily show key and value\n",
        "\n",
        "fruit = {\"apple\" : \"red\", \"orange\" : \"orange\", \"melon\" : \"green\"}\n",
        "\n",
        "for k, v in fruit.items():\n",
        "  print(\"{} has {} color\".format(k, v))\n",
        "  print(\"\")\n"
      ]
    }
